<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>Pos_system</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <Title>$(AssemblyName)</Title>
    <Company>Manuja Dayawansha</Company>
    <Description>This is simple pos system </Description>
    <RepositoryUrl>https://github.com/ManujaDayawansha/Pos-System.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>aa-ET</NeutralLanguage>
    <PackAsTool>True</PackAsTool>
    <ApplicationIcon>pos-logo.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="pos-logo.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.3719.77" />
  </ItemGroup>

</Project>